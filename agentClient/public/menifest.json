{
  "manifest_version": 3,
  "name": "AgentBinod Extension",
  "version": "1.0.0",
  "description": "A Chrome extension with React, Vite, and Tailwind CSS",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["storage", "activeTab"],
  "icons": {
    "128": "icon128.png"
  }
}
